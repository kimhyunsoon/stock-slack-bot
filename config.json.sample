{
  "comm": {
    "port": $SERVER_PORT
  },
  "db": {
    "host": $MONGODB_HOST,
    "port": $MONGODB_HOST_PORT,
    "password": $MONGODB_USER_PASSWORD,
    "user": $MONGODB_USER,
    "database": $MONGODB_DATABASE,
    "options": {}
  },
  "rest": {
    "key": $REST_KEY
  },
  "logs": {
    "path": "./logs",
    "loggableEventNames": [
    ]
  },
  "uploads": {
    "path": "./uploads"
  },
  "jwt": {
    "secret": $JWT_KEY,
    "options": {
      "access": {
        "algorithm": "HS512",
        "expiresIn": "7d"
      },
      "file": {
        "algorithm": "HS256",
        "expiresIn": "1h"
      }
    }
  }
  "slack": {
    "bot_token": $YOUR_SLACK_BOT_TOKEN,
    "user_token": $YOUR_SLACK_USER_TOKEN,
    "channel": $YOUR_SLACK_CHANNEL,
    "stock_url": $STOCK_PRICE_URL
  }
}
