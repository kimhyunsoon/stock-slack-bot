{
  "name": "micro-node",
  "version": "1.0.0",
  "main": "./src/app.ts",
  "license": "MIT",
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && nodemon --watch \"./src/**\" --ext \"ts,json,yaml\" --exec \"npx eslint ./src && ts-node ./src/app.ts\"",
    "inspect": "export NODE_OPTIONS=--openssl-legacy-provider && node --inspect-brk=9229 -r ts-node/register ./src/app.ts",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && tsc -p ."
  },
  "devDependencies": {
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/form-data": "^2.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.15.12",
    "@types/node-schedule": "^1.3.2",
    "@types/randomstring": "^1.1.8",
    "@types/useragent": "^2.3.1",
    "axios": "^1.3.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "events": "^3.3.0",
    "express": "^4.17.3",
    "image-size": "^1.0.1",
    "js-sha3": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^5.3.0",
    "mongoose": "^7.0.4",
    "node-schedule": "^2.1.0",
    "nodemon": "^2.0.22",
    "pino": "^7.8.0",
    "pino-pretty": "^10.0.0",
    "randomstring": "^1.2.2",
    "sha3": "^2.1.4",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.5.3",
    "typescript": "*",
    "useragent": "^2.3.0"
  }
}
